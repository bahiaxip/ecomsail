/* mensaje de error */


.message_modal
	background-color rgba(0,0,0,.5)
	display flex
	visibility hidden	
	justify-content center
	height 100vh
	opacity 0
	position absolute
	width 100%
	z-index 999
	transition all 0.3s linear
	.message.inflate
		animation-name inflate
		animation-duration 0.6s
		animation-iteration-count 1
		animation-timing-function ease-out
	.message
		/*background-color rgba(122,223,194,1)*/
		background-color #F3F3F3
		border-radius 8px
		display block
		min-height 170px
		margin auto
		min-width 500px
		padding 10px
		/*transform scale(0,0)*/
		/*transition transform 0.3s linear*/
		z-index 999
	
		
		/*animation-play-state paused*/
		div
			display flex
			margin auto
			min-height 50px
			width 100%
		
			h2
				align-items center
				display flex				
				font-family QuicksandB
				font-size 25px
				margin auto
			h3
				font-family QuicksandR
				font-size 18px
				margin auto
			i				
				font-size 2.5em
				margin auto
		
			
		div span
			display flex
			margin auto
			opacity 0
			/*transition all 1.5s ease-in-out*/
			
		div span.dnone
			display none
			
		div span.success
			transition transform 1.5s ease-in-out
			width 40px
			height 40px
			i
				color rgba(122,223,194,1)
				
		div span.danger
			i				
				color rgba(255,155,194,1)
		div span.info
			i
				color rgba(67,203,230,1)
		div span.warning
			i
				color color11
		
		div span.question
			i
				color color2
				
		div span.trash
			transition transform 1.5s ease-in-out
			background-color rgba(122,223,194,1)
			border-radius 50%
			width 40px
			height 40px
			i
				color #FFF
				font-size 1em
		
		div.buttons
			display flex
			justify-content space-around
			min-height 30px
			padding 10px

				
/*
.message_modal.modal_show
	visibility visible
	opacity 1			
*/

/* fin mensaje de error */
/* animación inflate: efector scale con rebote al final */
@keyframes inflate 
	0% 
		transform scale(0)
	
	50%
		transform scale(1)
	
	70%
		transform scale(0.85)
	
	80% 
		transform scale(1)
	
	90%
		transform scale(0.95)
	
	100% 
		transform scale(1)
	
